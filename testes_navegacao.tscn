[sub_resource type="GDScript" id="GDScript_qk2o4"]
resource_name = "tirar_stum"
script/source = "extends Area2D

# Variável para garantir que o código só execute uma vez
var ja_foi_ativado: bool = false

func _physics_process(_delta: float) -> void:
	if ja_foi_ativado:
		return

	var corpos = get_overlapping_bodies()
	for corpo in corpos:
		if corpo.is_in_group(\"player\"):
			ativar_evento(corpo)
			break # Sai do loop assim que achar o player

func ativar_evento(player_node: Node2D) -> void:
	ja_foi_ativado = true
	
	print(\"Player detectado no destino! Desativando área...\")
	
	# 1. Destrava o player
	if player_node.has_method(\"set_frozen\"):
		player_node.set_frozen(false)
	
	# 2. Desativa a detecção física da área para economizar processamento
	set_deferred(\"monitoring\", false)
	set_deferred(\"monitorable\", false)
	
	# 3. Opcional: Remove a área da cena se não for mais usá-la
	# queue_free()
"

